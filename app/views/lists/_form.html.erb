<%= simple_form_for @list do |f| %>

  <div class="input-field">
    <%= f.input :title %>
  </div>
  <p>
    <label for="contact_id">Please Some Contacts</label><br>
  <%= f.collection_check_boxes :contact_ids, @contacts, :id, 
        :name, :prompt => "Select Contacts", :wrapper => :inline_checkbox %>
  </p>


  <div class="input-field">
    <%= f.button :submit, class: "waves-effect waves-light teal lighten-1" %>
  </div>
<% end %>